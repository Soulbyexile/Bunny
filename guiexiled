local UIGradient = Instance.new("UIGradient")
local ScrollingFrame = Instance.new("ScrollingFrame")
local UIGradient_2 = Instance.new("UIGradient")
local _1Button = Instance.new("TextButton")
local UIGradient_3 = Instance.new("UIGradient")
local UICorner_2 = Instance.new("UICorner")
local _2Button = Instance.new("TextButton")
local UIGradient_4 = Instance.new("UIGradient")
local UICorner_3 = Instance.new("UICorner")
local _4Button = Instance.new("TextButton")
local UIGradient_5 = Instance.new("UIGradient")
local UICorner_4 = Instance.new("UICorner")
local _3Button = Instance.new("TextButton")
local UIGradient_6 = Instance.new("UIGradient")
local UICorner_5 = Instance.new("UICorner")
local UICorner_6 = Instance.new("UICorner")
local UICorner_7 = Instance.new("UICorner")
local UIGradient_7 = Instance.new("UIGradient")
local OpenMisc = Instance.new("TextButton")
local UICorner_8 = Instance.new("UICorner")
local UIGradient_8 = Instance.new("UIGradient")
local Misc = Instance.new("Frame")
local UICorner_9 = Instance.new("UICorner")
local UIGradient_9 = Instance.new("UIGradient")
local RightAlt = Instance.new("TextLabel")
local UICorner_10 = Instance.new("UICorner")
local UIGradient_10 = Instance.new("UIGradient")
local RightControl = Instance.new("TextLabel")
local UICorner_11 = Instance.new("UICorner")
local UIGradient_11 = Instance.new("UIGradient")

--Properties:

Misc.Visible = false
CloseMisc.Visible = false

ScreenGui.Parent = game.CoreGui

Main.Name = "Main"
Main.Parent = ScreenGui
Main.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Main.BorderColor3 = Color3.fromRGB(27, 42, 53)
Main.BorderSizePixel = 0
Main.Position = UDim2.new(0, 0, 0.254158556, 0)
Main.Size = UDim2.new(0, 475, 0, 268)
Main.Draggable = true
Main.Active = true
--CLOSE GUI--
game:GetService("UserInputService").InputBegan:Connect(function(input, GameProcessedEvent)
	if input.UserInputType == Enum.UserInputType.Keyboard then
		if input.KeyCode == Enum.KeyCode.RightControl then
			if Main.Visible == true then
				Main.Visible = false
				Misc.Visible = false
				CloseMisc.Visible = false
			else
				Main.Visible = true
			end
			end
		end
end)


--TRANSPERENT GUI--
game:GetService("UserInputService").InputBegan:Connect(function(input, GameProcessedEvent)
	if input.UserInputType == Enum.UserInputType.Keyboard then
		if input.KeyCode == Enum.KeyCode.RightAlt then
			if Main.BackgroundTransparency == 0 then
				Main.BackgroundTransparency = 0.5
				Misc.BackgroundTransparency = 0.5
				ScrollingFrame.BackgroundTransparency = 0.5
			else
				Main.BackgroundTransparency = 0
				Misc.BackgroundTransparency = 0
				ScrollingFrame.BackgroundTransparency = 0
			end
		end
	end
end)

CloseMisc.Name = "CloseMisc"
CloseMisc.Parent = Main
CloseMisc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
CloseMisc.Position = UDim2.new(0.189878196, 0, 0.935891211, 0)
CloseMisc.Size = UDim2.new(0, 23, 0, 17)
CloseMisc.ZIndex = 5
CloseMisc.Font = Enum.Font.SourceSansBold
CloseMisc.Text = "X"
CloseMisc.TextColor3 = Color3.fromRGB(0, 0, 0)
CloseMisc.TextSize = 18.000
CloseMisc.MouseButton1Down:connect(function()
	Misc.Visible = false
	CloseMisc.Visible = false
end)

UICorner.CornerRadius = UDim.new(0, 4)
UICorner.Parent = CloseMisc

UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(224, 43, 39)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(168, 3, 3))}
UIGradient.Parent = CloseMisc

